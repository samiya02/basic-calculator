#include<stdio.h>
int main(){
  char x;
  int a=20,b=10;
  printf("enter an operator\n");
  scanf("%c",&x);
  if(x=='+'){
    printf("%d\n",a+b);
  }else if(x=='-'){
    printf("%d\n",a-b);
  }else if(x=='*'){
    printf("%d\n",a*b);
  }else if(x=='/'){
    printf("%d\n",a/b);
  }else if(x=='%'){
    printf("%d\n",a%b);
  }else{
    printf("invalid operator");
  }
  return 0;
}
